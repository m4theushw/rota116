<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,600" rel="stylesheet">
    <title>Rota 116</title>
    <style>
      body {
        margin: 0;
        padding: 60px 0 20px;
        text-align: center;
        background-color: #efefef;
        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
      }

      .backends {
        margin: 50px auto;
      }

      .backends form {
        display: inline-block;
      }

      .backends button {
        width: 120px;
        margin: 0 10px;
        font-size: 1rem;
        border-radius: 4px;
        padding: 10px 15px;
        text-transform: uppercase;
        border: 2px solid #2c2c2c;
        background-color: #ffd500;
      }

      .backends button.selected {
        font-weight: bold;
      }
    </style>
  </head>
  <body>
    <img src="data:image/png;base64,">
    <div class="backends">
      <h3>Which backend to use?</h3>
      <% backends.forEach(function(backend) { %>
      <form method="POST">
        <input type="hidden" name="backend" value="<%= backend %>" />
        <button type="submit"<%- selectedBackend == backend ? ' class="selected"' : '' %>><%= backend %></button>
      </form>
      <% }) %>
    </div>
    <p>All requests to <strong><%= currentUrl %></strong> will be routed to <strong><%= backendUrl %></strong>.</p>
  </body>
</html>
